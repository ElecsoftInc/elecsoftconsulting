<!DOCTYPE html>
<html>
<head>
    <title>Admin Login ~ Elecsoft Consulting</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" href="/assets/css/main.css" />
</head>
<body>

 <!-- Form -->
    <section style="padding-left: 30em;">
      <h3>Form</h3>
      <form method="post" action="/adminDash">
        <div class="row uniform 100%">
          <div class="6u$ 12u$(xsmall)">
            <input type="email" name="email" id="email" value="" placeholder="Email" />
          </div>
          <div class="6u 12u$(xsmall)">
            <input type="password" name="password" id="name" value="" placeholder="Password" />
          </div>
          <div class="12u$">
            <ul class="actions">
              <li><input type="submit" value="Enter" class="special" /></li>
            </ul>
          </div>
        </div>
      </form>
    </section>


    <!-- Scripts -->
  <script src="/assets/js/jquery.min.js"></script>
  <script src="/assets/js/skel.min.js"></script>
  <script src="/assets/js/util.js"></script>
  <script src="/assets/js/main.js"></script>

</body>
</html>
